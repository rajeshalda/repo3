{
  "name": "meeting-time-tracker",
  "version": "2.2.2",
  "private": true,
  "scripts": {
    "predev": "node scripts/pm2-check.js",
    "dev": "cross-env START_PM2=true next dev -p 8080",
    "build": "next build",
    "start": "cross-env NODE_ENV=production npm run pm2:install && npm run build && npm run pm2:start && next start -p 8080",
    "lint": "next lint",
    "pm2:install": "npm install pm2 -g",
    "pm2:start": "pm2 start pm2.config.js",
    "pm2:stop": "pm2 stop ai-agent",
    "pm2:restart": "pm2 restart ai-agent",
    "pm2:status": "pm2 status",
    "pm2:logs": "pm2 logs ai-agent",
    "pm2:monitor": "pm2 monit",
    "pm2:dashboard": "pm2 plus",
    "pm2:save": "pm2 save",
    "pm2:startup": "pm2 startup",
    "setup": "npm install && npm run pm2:install",
    "dev:with-agent": "npm run pm2:start && npm run dev",
    "dev:no-agent": "cross-env START_PM2=false next dev -p 8080"
  },
  "dependencies": {
    "@azure/identity": "^4.6.0",
    "@azure/msal-browser": "^4.0.1",
    "@azure/msal-node": "^3.0.1",
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "@radix-ui/react-avatar": "^1.1.2",
    "@radix-ui/react-checkbox": "^1.1.3",
    "@radix-ui/react-dialog": "^1.1.13",
    "@radix-ui/react-dropdown-menu": "^2.1.5",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-popover": "^1.1.4",
    "@radix-ui/react-progress": "^1.1.1",
    "@radix-ui/react-scroll-area": "^1.2.8",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-switch": "^1.1.3",
    "@radix-ui/react-tabs": "^1.1.2",
    "@radix-ui/react-toast": "^1.2.4",
    "axios": "^1.8.4",
    "better-sqlite3": "^11.10.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^0.2.1",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.7",
    "limiter": "^3.0.0",
    "lucide-react": "^0.472.0",
    "luxon": "^3.6.1",
    "next": "15.1.5",
    "next-auth": "^4.24.11",
    "next-themes": "^0.4.4",
    "react": "^19.0.0",
    "react-day-picker": "^8.10.1",
    "react-dom": "^19.0.0",
    "react-markdown": "^10.1.0",
    "sonner": "^1.7.2",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/better-sqlite3": "^7.6.13",
    "@types/cookie": "^0.6.0",
    "@types/luxon": "^3.6.2",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "eslint": "^9",
    "eslint-config-next": "15.1.5",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
